
    {
      "id": 114,
      "name": {
        "AR": "الناس",
        "EN": ["An-Naas", "Mankind"]
      },
      "versesLength": 6,
      "verses": [{
            "AR_text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ قُلْ أَعُوذُ بِرَبِّ ٱلنَّاسِ",
            "tafseer": "قل -أيها الرسول-: أعوذ وأعتصم برب الناس، القادر وحده على ردِّ شر الوسواس.",
            "EN_text": "Say, 'I seek refuge in the Lord of mankind,",
            "tajweed": [{
            "from": 60,
            "to": 61,
            "rule": "hamzat_wasl"
          },{
            "from": 61,
            "to": 62,
            "rule": "lam_shamsiyyah"
          },{
            "from": 62,
            "to": 65,
            "rule": "ghunnah"
          },{
            "from": 65,
            "to": 66,
            "rule": "madd_246"
          }]
          },{
            "AR_text": "مَلِكِ ٱلنَّاسِ",
            "tafseer": "ملك الناس المتصرف في كل شؤونهم، الغنيِّ عنهم.",
            "EN_text": "The Sovereign of mankind.",
            "tajweed": [{
            "from": 7,
            "to": 8,
            "rule": "hamzat_wasl"
          },{
            "from": 8,
            "to": 9,
            "rule": "lam_shamsiyyah"
          },{
            "from": 9,
            "to": 12,
            "rule": "ghunnah"
          },{
            "from": 12,
            "to": 13,
            "rule": "madd_246"
          }]
          },{
            "AR_text": "إِلَٰهِ ٱلنَّاسِ",
            "tafseer": "إله الناس الذي لا معبود بحق سواه.",
            "EN_text": "The God of mankind,",
            "tajweed": [{
            "from": 4,
            "to": 5,
            "rule": "madd_2"
          },{
            "from": 8,
            "to": 9,
            "rule": "hamzat_wasl"
          },{
            "from": 9,
            "to": 10,
            "rule": "lam_shamsiyyah"
          },{
            "from": 10,
            "to": 13,
            "rule": "ghunnah"
          },{
            "from": 13,
            "to": 14,
            "rule": "madd_246"
          }]
          },{
            "AR_text": "مِن شَرِّ ٱلْوَسْوَاسِ ٱلْخَنَّاسِ",
            "tafseer": "من أذى الشيطان الذي يوسوس عند الغفلة، ويختفي عند ذكر الله.",
            "EN_text": "From the evil of the retreating whisperer -",
            "tajweed": [{
            "from": 2,
            "to": 6,
            "rule": "ikhfa"
          },{
            "from": 10,
            "to": 11,
            "rule": "hamzat_wasl"
          },{
            "from": 23,
            "to": 24,
            "rule": "hamzat_wasl"
          },{
            "from": 28,
            "to": 31,
            "rule": "ghunnah"
          },{
            "from": 31,
            "to": 32,
            "rule": "madd_246"
          }]
          },{
            "AR_text": "ٱلَّذِى يُوَسْوِسُ فِى صُدُورِ ٱلنَّاسِ",
            "tafseer": "الذي يبثُّ الشر والشكوك في صدور الناس.",
            "EN_text": "Who whispers [evil] into the breasts of mankind -",
            "tajweed": [{
            "from": 31,
            "to": 32,
            "rule": "hamzat_wasl"
          },{
            "from": 32,
            "to": 33,
            "rule": "lam_shamsiyyah"
          },{
            "from": 33,
            "to": 36,
            "rule": "ghunnah"
          },{
            "from": 36,
            "to": 37,
            "rule": "madd_246"
          }]
          },{
            "AR_text": "مِنَ ٱلْجِنَّةِ وَٱلنَّاسِ",
            "tafseer": "من شياطين الجن والإنس.",
            "EN_text": "From among the jinn and mankind.'",
