      "versesLength": 5,
      "verses": [{
            "AR_text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ قُلْ أَعُوذُ بِرَبِّ ٱلْفَلَقِ",
            "tafseer": "قل -أيها الرسول-: أعوذ وأعتصم برب الفلق، وهو الصبح.",
            "EN_text": "Say, 'I seek refuge in the Lord of daybreak",
            "tajweed": [{
            "from": 60,
            "to": 61,
            "rule": "hamzat_wasl"
          },{
            "from": 67,
            "to": 68,
            "rule": "qalqalah"
          }]
          },{
            "AR_text": "مِن شَرِّ مَا خَلَقَ",
            "tafseer": "من شر جميع المخلوقات وأذاها.",
            "EN_text": "From the evil of that which He created",
            "tajweed": [{
            "from": 2,
            "to": 6,
            "rule": "ikhfa"
          },{
            "from": 18,
            "to": 19,
            "rule": "qalqalah"
          }]
          },{
            "AR_text": "وَمِن شَرِّ غَاسِقٍ إِذَا وَقَبَ",
            "tafseer": "ومن شر ليل شديد الظلمة إذا دخل وتغلغل، وما فيه من الشرور والمؤذيات.",
            "EN_text": "And from the evil of darkness when it settles",
            "tajweed": [{
            "from": 4,
            "to": 8,
            "rule": "ikhfa"
          },{
            "from": 30,
            "to": 31,
            "rule": "qalqalah"
          }]
          },{
            "AR_text": "وَمِن شَرِّ ٱلنَّفَّٰثَٰتِ فِى ٱلْعُقَدِ",
            "tafseer": "ومن شر الساحرات اللاتي ينفخن فيما يعقدن من عُقَد بقصد السحر.",
            "EN_text": "And from the evil of the blowers in knots",
            "tajweed": [{
            "from": 4,
            "to": 8,
            "rule": "ikhfa"
          },{
            "from": 12,
            "to": 13,
            "rule": "hamzat_wasl"
          },{
